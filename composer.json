{
    "name": "fabpot/silex-skeleton",
    "description": "A pre-configured skeleton for the Silex microframework",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.3.3",
        "silex/silex": "~1.0",
        "silex/web-profiler": "~1.0",
        "symfony/browser-kit": "~2.3",
        "symfony/class-loader": "~2.3",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/css-selector": "~2.3",
        "symfony/debug": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/monolog-bridge": "~2.3",
        "symfony/process": "~2.3",
        "symfony/security": "^3.0",
        "symfony/translation": "~2.6",
        "symfony/twig-bridge": "~2.3",
        "symfony/validator": "~2.3",
        "twig/extensions": "^1.3",
        "vlucas/phpdotenv": "^2.1",
        "doctrine/dbal": "2.5.1",
        "symfony/http-kernel": "^2.8",
        "dflydev/doctrine-orm-service-provider" : "1.0.*",
        "mheap/Silex-Assetic": "1.*",
        "natxet/CssMin": "3.*",
        "linkorb/jsmin-php": "dev-master"
    },
    "autoload": {
        "psr-0": { "": "app/" },
        "psr-4": { "Core\\": "app/core", "Src\\": "app/src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
